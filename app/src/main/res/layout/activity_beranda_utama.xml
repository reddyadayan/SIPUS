<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnlocation"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowlocation"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLocation"
                    android:layout_height="@dimen/_8pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_location"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                <ImageView android:id="@+id/imageSignal"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_305pxh"
                    android:layout_width="@dimen/_59pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackunion"
                android:layout_height="1185dp" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageUnion"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_387pxh"
                    android:layout_marginBottom="@dimen/_346pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_346pxv"
                    android:layout_width="@dimen/_406pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_union}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                <FrameLayout android:id="@+id/frameStackunionOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_713pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_385pxh">
                    <ImageView android:id="@+id/imageUnionOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_367pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_union_light_blue_a700}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_union_light_blue_a700"/>
                    <LinearLayout
                        android:id="@+id/linearColumnsmartcity"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_30pxh"
                        android:layout_marginVertical="@dimen/_30pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtSmartCity"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_smart_city"
                            style="@style/txtInterextrabold75" tools:text="@string/lbl_smart_city"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGrowTogether"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_grow_together"
                            style="@style/txtIntermedium20" tools:text="@string/lbl_grow_together"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnlanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_45pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStyleblack_900cornerRadius">
                            <TextView
                                android:gravity="center_horizontal"
                                android:id="@+id/txtLanguage"
                                android:layout_height="match_parent"
                                android:layout_marginEnd="@dimen/_51pxh"
                                android:layout_marginStart="@dimen/_51pxh"
                                android:layout_width="match_parent"
                                android:text="@string/lbl_daftar_sekarang"
                                style="@style/txt" tools:text="@string/lbl_daftar_sekarang"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <ImageView android:id="@+id/imageSIPUSOne"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_591pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_388pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_sipus1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_sipus1"/>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
