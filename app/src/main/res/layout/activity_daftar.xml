<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnlocation"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStacklocation"
            android:layout_height="@dimen/_185pxv" android:layout_width="match_parent">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowlocation"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                    <ImageView android:id="@+id/imageSignal"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_305pxh"
                        android:layout_width="@dimen/_59pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_signal"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:gravity="top|start"
                android:id="@+id/txtBuatAccount"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_buat_account"
                style="@style/txtInterbold32" tools:text="@string/lbl_buat_account"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_teal_300_radius_18"
                android:id="@+id/btnArrowleft"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginBottom="@dimen/_64pxv"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_64pxv"
                android:layout_width="@dimen/_36pxh"
                android:paddingHorizontal="@dimen/_12pxh"
                android:paddingVertical="@dimen/_12pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
            <ImageView android:id="@+id/imageSIPUSThree"
                android:layout_gravity="start"
                android:layout_height="@dimen/_150pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_150pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_sipus3}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_sipus3"/>
        </FrameLayout>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_76pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearDaftar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_masukan_email"
                    style="@style/txtIntermedium26" tools:text="@string/lbl_masukan_email"/>
                <View android:background="@color/black_900"
                    android:id="@+id/lineLineThree"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_51pxv" android:layout_width="@dimen/_349pxh"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtMasukanUsernam"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_45pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_masukan_usernam2"
                    style="@style/txtIntermedium26" tools:text="@string/msg_masukan_usernam2"/>
                <View android:background="@color/black_900"
                    android:id="@+id/lineLineOne"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_52pxv" android:layout_width="@dimen/_349pxh"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtMasukanPasswor"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_42pxh"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_58pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_masukan_passwor"
                    style="@style/txtIntermedium26" tools:text="@string/msg_masukan_passwor"/>
                <FrameLayout android:id="@+id/frameStackunion"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_729pxv"
                    android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_388pxh">
                    <ImageView android:id="@+id/imageUnion"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_373pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_322pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_union}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                    <FrameLayout android:id="@+id/frameStackunionOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_713pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_305pxh">
                        <ImageView android:id="@+id/imageUnionOne"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_357pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_union_light_blue_a700}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_union_light_blue_a700"/>
                        <LinearLayout
                            android:id="@+id/linearColumnpunyaakun"
                            android:layout_gravity="top|end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_197pxv"
                            android:layout_marginEnd="@dimen/_43pxh"
                            android:layout_marginStart="@dimen/_43pxh"
                            android:layout_marginTop="@dimen/_197pxv"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtPunyaAkun"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@string/lbl_punya_akun"
                                style="@style/txtInterregular15" tools:text="@string/lbl_punya_akun"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLogin"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_26pxh"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_login"
                                style="@style/txtInterextrabold15" tools:text="@string/lbl_login"/>
                        </LinearLayout>
                    </FrameLayout>
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLineTwo"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_50pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_50pxv" android:layout_width="@dimen/_349pxh"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnDaftar"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_54pxv"
                        android:layout_marginBottom="@dimen/_137pxv"
                        android:layout_marginEnd="@dimen/_57pxh"
                        android:layout_marginStart="@dimen/_57pxh"
                        android:layout_marginTop="@dimen/_137pxv"
                        android:layout_width="@dimen/_229pxh"
                        android:paddingBottom="@dimen/_15pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_15pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_daftar"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_daftar"/>
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
