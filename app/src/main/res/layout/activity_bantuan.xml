<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameBantuan"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnlocation"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvector"
                android:layout_height="@dimen/_833pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnvector"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_100">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_43pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTentangKami"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_weight="0.75"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_tentang_kami"
                            style="@style/txtInterregular24" tools:text="@string/lbl_tentang_kami"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvectorOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_585pxv"
                        android:layout_marginEnd="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_44pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_44x50"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_44x50"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtKebijakanpriba"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_weight="0.79"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/msg_kebijakan_priba"
                            style="@style/txtInterregular24" tools:text="@string/msg_kebijakan_priba"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
                    <LinearLayout android:id="@+id/linearRowlocation"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLocation"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                        <ImageView android:id="@+id/imageSignal"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_305pxh"
                            android:layout_width="@dimen/_59pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_signal"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_56pxv"
                    android:layout_marginTop="@dimen/_56pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleteal_300">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_yellow_a200_radius_18"
                        android:id="@+id/btnArrowleft"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingHorizontal="@dimen/_12pxh"
                        android:paddingVertical="@dimen/_12pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowleft_teal_300" tools:ignore="ContentDescription"/>
                    <LinearLayout android:id="@+id/linearRowvectorTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_168pxh"
                        android:layout_marginStart="@dimen/_72pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_24x26"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_24x26"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBantuan"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.78"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_bantuan"
                            style="@style/txtSairaromanbold24" tools:text="@string/lbl_bantuan"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageSIPUSFour"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_100pxh"
                    android:layout_marginBottom="@dimen/_37pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="@dimen/_75pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_sipus4_2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_sipus4_2"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowcopyrighticon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_128pxh"
                android:layout_marginStart="@dimen/_128pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCopyrighticon"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_copyrighticon}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_copyrighticon"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCopyright2023S"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/msg_copyright2023_s"
                    style="@style/txtIntermedium14" tools:text="@string/msg_copyright2023_s"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
